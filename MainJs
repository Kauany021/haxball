var fetchUrl = require("fetch").fetchUrl;


// CSS DESIGN
const gameDoc = document.getElementsByClassName('gameframe')[0].contentWindow.document;

fetchUrl("https://raw.githubusercontent.com/Kauany021/haxball/main/Design", function(error, meta, body){
    if (body && body.toString() != "") {
        let designScript = body.toString();
        gameDoc.head.insertAdjacentHTML("beforeend", designScript);
    }
});

// REMOVE HEADER
document.querySelector("body > div > div.header").remove();

gameDoc.querySelector("#wrapper").remove();



